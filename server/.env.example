# Server Configuration
PORT=3000
CACHE_TTL_MS=300000

# Google Maps API Key
# Get yours from: https://console.cloud.google.com/google/maps-apis
GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here

# Instagram API (Optional - for social feed feature)
# Get token from: https://developers.facebook.com/docs/instagram-basic-display-api
INSTAGRAM_ACCESS_TOKEN=your_instagram_token_here
INSTAGRAM_USER_ID=your_instagram_user_id_here

# Admin Registration Secret Key
# Choose a strong, random string for production
ADMIN_SECRET_KEY=your_secure_admin_key_here
